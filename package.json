  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:cordova": "cd cordova && cordova build android",
    "build:ios": "node scripts/build.js && cd cordova && cordova build ios",
    "android": "node scripts/build.js && cd cordova && cordova run android",
    "test": "node scripts/test.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "strict": 0,
      "eqeqeq": 0,
      "no-mixed-operators": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "no-unused-vars": 2,
      "no-dupe-keys": 2,
      "no-var": "error",
      "init-declarations": 0,
      "no-useless-concat": 0,
      "comma-spacing": 2,
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-extra-semi": "error",
      "semi": [
        "error",
        "never"
      ],
      "linebreak-style": [
        "off",
        "windows"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "block-spacing": [
        "error",
        "always"
      ],
      "key-spacing": [
        2,
        {
          "beforeColon": false,
          "afterColon": true
        }
      ],
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "arrow-spacing": 2,
      "keyword-spacing": 2,
      "space-before-blocks": 2,
      "array-callback-return": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd-mobile",
          "style": "css"
        }
      ]
    ]
  }
}
